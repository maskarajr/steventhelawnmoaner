{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "blob": {
    "enabled": true
  },
  "crons": [{
    "path": "/api/cron",
    "schedule": "*/15 * * * *"
  }]
} 